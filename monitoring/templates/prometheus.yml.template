global:
  scrape_interval: ${PROMETHEUS_SCRAPE_INTERVAL}
  scrape_timeout: 5s
  evaluation_interval: 15s

scrape_configs:
  - job_name: "prometheus"
    static_configs:
      - targets: ["127.0.0.1:9090"]

  - job_name: "hostmetrics"
    static_configs:
      - targets: ["127.0.0.1:9105"]

  - job_name: "relaymetrics"
    static_configs:
      - targets: ["10.154.0.18:9105"]      
