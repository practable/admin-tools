#!/bin/sh
mkdir -p ./autogenerated 
cd ./autogenerated

# example: relayaccess https://relay-access.practable.io pvna 32 34 st-ed0- #makes files for pvna32, pvna33, and pvna34 with streams st-ed0-data, st-ed0-video

for i in $(seq $3 $4)
do
    num=$(printf "%02d" $i)
	name="${2}${num}"
	echo "${1}/session/${name}-st-data" > "${5}data.access.${name}"  
	echo "${1}/session/${name}-st-video" > "${5}video.access.${name}"
done




